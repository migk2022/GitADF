						"referenceName": "Salva_arquivo_output",
						"type": "DatasetReference",
						"parameters": {
							"output_filename": {
								"value": "\"Sales_Derive_output.csv\"",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"arquivoorigem": {
				"type": "string"
